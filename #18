# 18. BÃºsqueda binaria
lista = [1, 3, 5, 7, 9]  # Lista ordenada
objetivo = 5
inicio, fin = 0, len(lista) - 1
encontrado = False
while inicio <= fin:
    medio = (inicio + fin) // 2
    if lista[medio] == objetivo:
        encontrado = True
        resultado = 'si'
        break
    elif lista[medio] < objetivo:
        inicio = medio + 1
    else:
        fin = medio - 1
print(f"El numero {resultado} fue encontrado: {objetivo}")
